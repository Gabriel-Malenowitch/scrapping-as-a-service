{
  "name": "puppeteer-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "docker:build": "docker build -t scrapping-as-a-service .",
    "docker:run": "docker run -p 5500:5500 scrapping-as-a-service",
    "pull": "git pull && yarn && docker rm -f $(docker ps -aq) && yarn docker:build && yarn docker:run",
    "restart": "yarn && docker rm -f $(docker ps -aq) && yarn docker:build && yarn docker:run"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "express": "^4.18.2",
    "puppeteer": "^21.5.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
